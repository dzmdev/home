{
  "name": "home",
  "private": true,
  "version": "1.0.0-*",
  "compilationOptions": {
    "emitEntryPoint": true
  },

  "dependencies": {
    "Microsoft.AspNet.IISPlatformHandler": "1.0.0-rc1-final",
    "Microsoft.AspNet.Mvc": "6.0.0-rc1-final",
    "Microsoft.AspNet.Server.Kestrel": "1.0.0-rc1-final",
    "Microsoft.AspNet.StaticFiles": "1.0.0-rc1-final"
  },

  "commands": {
    "web": "Microsoft.AspNet.Server.Kestrel"
  },

  "scripts": {
    "prepare": "npm install",
    "prebuild": "npm run build",
    "publish": [ "dnvm install latest -a x64 -os win", "dnu publish  -o ..\\..\\bin --runtime active" ],
    "clean": "npm run clean"
  },

  "-vs-binding": {
    "BeforeBuild": [ "prebuild" ],
    "Clean": [ "clean" ]
  },

  "frameworks": {
    "dnx451": { },
    "dnxcore50": { }
  },

  "publishExclude": [
    "**.user",
    "gruntfile.js",
    "package.json",
    "**/Views/**.js",
    "**/Views/**.ts",
    "**.vspscc"
  ]
}
